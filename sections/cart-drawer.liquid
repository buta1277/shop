<script>
  class LoadingBar extends HTMLElement {
    constructor() {
      super();
      const shadow = this.attachShadow({ mode: 'open' });

      shadow.innerHTML = `
        <style>
          :host {
            display: block;
            height: 4px;
            background: transparent;
            position: relative;
          }

          .wrapper {
            width: 100%;
            height: 100%;
            background: #eee;
            overflow: hidden;
          }

          .bar {
            width: 100%;
            height: 100%;
            background: #4caf50;
            transform: scaleX(0);
            transform-origin: left;
            transition: transform 0.1s linear;
          }

          :host([hidden]) {
            display: none;
          }
        </style>
        <div class="wrapper">
          <div class="bar"></div>
        </div>
      `;

      this.bar = shadow.querySelector('.bar');
      this._progress = 0;
      this._running = false;
      this._raf = null;
    }

    set(percent) {
      this._progress = Math.max(0, Math.min(percent, 100));
      const scale = this._progress / 100;
      this.bar.style.transform = `scaleX(${scale})`;
      this.removeAttribute('hidden');
    }

    start() {
      this._progress = 0;
      this.set(0);
      this._running = true;
      this.removeAttribute('hidden');

      const animate = () => {
        if (!this._running) return;

        const target = 90;
        const easing = 0.015; // nhỏ hơn → chậm dần

        this._progress += (target - this._progress) * easing;
        const scale = this._progress / 100;
        this.bar.style.transform = `scaleX(${scale})`;

        this._raf = requestAnimationFrame(animate);
      };

      this._raf = requestAnimationFrame(animate);
    }

    end() {
      this._running = false;
      cancelAnimationFrame(this._raf);

      this.set(100);

      setTimeout(() => {
        this.setAttribute('hidden', '');
        this.set(0); // reset để dùng lại
      }, 500);
    }
  }
  customElements.define('loading-bar', LoadingBar);
</script>
